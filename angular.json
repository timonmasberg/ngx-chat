{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ngx-chat-demo": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "demo",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "allowedCommonJsDependencies": [
              "@xmpp/jid",
              "@xmpp/client"
            ],
            "outputPath": "dist/ngx-chat-demo",
            "index": "demo/index.html",
            "main": "demo/main.ts",
            "polyfills": "demo/polyfills.ts",
            "tsConfig": "demo/tsconfig.app.json",
            "assets": [],
            "styles": [
              "demo/styles.css"
            ],
            "scripts": [],
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "demo/environments/environment.ts",
                  "with": "demo/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            },
            "jsxc": {
              "fileReplacements": [
                {
                  "replace": "demo/environments/environment.ts",
                  "with": "demo/environments/environment.jsxc.ts"
                }
              ]
            },
            "converse": {
              "fileReplacements": [
                {
                  "replace": "demo/environments/environment.ts",
                  "with": "demo/environments/environment.converse.ts"
                }
              ]
            },
            "pazz": {
              "fileReplacements": [
                {
                  "replace": "demo/environments/environment.ts",
                  "with": "demo/environments/environment.pazz.ts"
                }
              ]
            }
          },
          "defaultConfiguration": ""
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "ngx-chat-demo:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "ngx-chat-demo:build:production"
            },
            "jsxc": {
              "browserTarget": "ngx-chat-demo:build:jsxc"
            },
            "converse": {
              "browserTarget": "ngx-chat-demo:build:converse"
            },
            "pazz": {
              "browserTarget": "ngx-chat-demo:build:pazz"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "demo/tsconfig.app.json",
              "tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
      }
    },
    "@pazznetwork/ngx-chat": {
      "projectType": "library",
      "root": "",
      "sourceRoot": "src",
      "prefix": "ngx",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "tsconfig.lib.json",
            "project": "ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.lib.json",
              "tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "dist"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "ngx-chat-demo"
}
